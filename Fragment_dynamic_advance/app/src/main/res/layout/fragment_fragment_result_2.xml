<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.hungyuwei.fragment_static.FragmentResult_2">

   <TableRow>
       <TextView
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:text="@string/total_set"
           android:textSize="6sp"/>

       <EditText
           android:id="@+id/edtCountSet"
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:textSize="6sp"/>
   </TableRow>

    <TableRow>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/computer_win_set"
            android:textSize="6sp"/>

        <EditText
            android:id="@+id/edtCountComWin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="6sp"/>
    </TableRow>

    <TableRow>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/player_win_set"
            android:textSize="6sp"/>

        <EditText
            android:id="@+id/edtCountPlayerWin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="6sp"/>
    </TableRow>

    <TableRow>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/draw_set"
            android:textSize="6sp"/>

        <EditText
            android:id="@+id/edtCountDraw"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="6sp"/>
    </TableRow>

</TableLayout>
